<main role="main">
    <div class="album py-3 bg-light">
        <div class="container">
            <div class="row">
                <div class="col-md-3 project-detail-sb">
                    <div class="project-sidebar-block mb-4">
                        <h2>Links</h2>
                        <div class="project-sidebar-links">
                            <a class="btn btn-primary" href="/project/{{project.alias}}/demo">Live Demo</a>
                        </div>
                        <div class="project-sidebar-links">
                            <a href="{{project.gitHubLink}}" class="btn btn-secondary">Github Link</a>
                        </div>
                    </div>
                    <hr>
                    <div class="project-sidebar-block mb-4">
                        <h2>Tags</h2>
                        <div class="project-sidebar-links">
                            <a class="badge badge-primary" href="#">Javascript</a>
                            <a class="badge badge-danger" href="#">Random</a>
                            <a class="badge badge-success" href="#">Math</a>
                        </div>
                    </div>
                    <hr>
                    <div class="project-sidebar-block mb-4">
                        <h2>Related Projects</h2>
                        <div class="project-sidebar-links">
                            <div class="project-sidebar-links">
                                <a class="bc-item" href="#">Javascript</a>
                            </div>
                            <div class="project-sidebar-links">
                                <a class="bc-item" href="#">Javascript</a>
                            </div>
                            <div class="project-sidebar-links">
                                <a class="bc-item" href="#">Javascript</a>
                            </div>
                            <div class="project-sidebar-links">
                                <a class="bc-item" href="#">Javascript</a>
                            </div>
                        </div>
                    </div>
                    <div class="project-sidebar-block">
                        <h2>Resources</h2>
                        <div class="project-sidebar-links">
                            <div class="project-sidebar-links">
                                <a href="/projects/" class="bc-item">JS Console</a>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="col-md-9">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item bc-item" >Home</li>
                            <li class="breadcrumb-item bc-item">Projects</li>
                            <li class="breadcrumb-item" aria-current="page">{{project.name}}</li>
                        </ol>
                    </nav>
                    <h1 class="display-3">{{ project.name }}</h1>
                    <br>
                    <p>{{project.description}}</p>
                    <br>
                    <div id="projectSlider" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            
                            <div class="carousel-item active">
                                <img class="d-block w-100" src="/image/ashu.JPG" alt="Slide 1">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="/image/logo.png" alt="Slide 1">
                            </div>
                           
                        </div>
                        <a class="carousel-control-prev" href="#projectSlider" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#projectSlider" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                    <br>
                    <h2>Watch a Live Demo <a class="bc-item" href="#">here</a></h2>
                </div>
            </div>
        </div>
    </div>

</main>