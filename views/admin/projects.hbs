<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Project</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
            <div class="btn-group mr-2">
                <a class="btn btn-sm btn-outline-secondary">Create New</a>
                <a class="btn btn-sm btn-outline-secondary">Upload</a>
            </div>
        </div>
    </div>

    <div class="table-responsive">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>Image</th>
                    <th>Name</th>
                    <th>Alias</th>
                    <th>Github</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                {{#each projects}}
                <tr>
                    <td><a href="#"><img width="100" height="100" src="{{this.image}}" alt="" srcset=""></a></td>
                    <td><a href=#>{{this.name}}</a></td>
                    <td>{{this.alias}}</td>
                    <td><a href="{{this.githubUrl}}">Repo</a></td>
                    <td>
                        <a href="#">
                            <span data-feather="alert-circle"></span>
                        </a>
                          <a href="#">
                            <span data-feather="x-circle"></span>
                        </a>
                        <a href="#">
                            <span data-feather="copy"></span>
                        </a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</main>